<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>
    <style name="ProgressBar" parent="android:style/Widget.Holo.ProgressBar">
        <item name="android:indeterminateDrawable">@drawable/progressbar</item>
        <item name="android:indeterminateDuration">50</item>
    </style>
    <style name="ProgressBar2" parent="android:style/Widget.Holo.ProgressBar">
        <item name="android:indeterminateDrawable">@drawable/progressbar2</item>
        <item name="android:indeterminateDuration">200</item>
    </style>
</resources>