<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <!--注意名称 -->
    <com.jerry.wuwen.resource.LineTextView

        android:id="@+id/lineTextView"
        style="?android:attr/textViewStyle"
        android:layout_width="359dp"
        android:layout_height="90dp"
        android:background="@null"
        android:textColor="@null"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="99dp">


        <com.jerry.wuwen.resource.LineEditText
            android:id="@+id/myEdit2"
            style="?android:attr/textViewStyle"
            android:layout_width="246dp"
            android:layout_height="39dp"
            android:layout_marginTop="44dp"
            android:background="@null"
            android:hint="@string/login_username"
            android:textColor="@null"
            android:textCursorDrawable="@drawable/my_cursor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.698"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="38dp"
            android:layout_height="43dp"
            android:layout_marginEnd="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/myEdit2"
            app:layout_constraintEnd_toStartOf="@+id/myEdit2"
            app:layout_constraintTop_toTopOf="@+id/myEdit2"
            app:layout_constraintVertical_bias="0.714"
            app:srcCompat="@android:drawable/ic_dialog_email" />

        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="4dp"
            android:background="#FF909090"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/imageView2"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.666" />
    </com.jerry.wuwen.resource.LineTextView>


</androidx.constraintlayout.widget.ConstraintLayout>